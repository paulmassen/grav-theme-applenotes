{
  "name": "apple-notes-theme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=development tailwindcss -i ./css/source.css -o ./css/apple-notes.css --postcss",
    "watch": "cross-env NODE_ENV=development tailwindcss -i ./css/source.css -o ./css/apple-notes.css --postcss --watch",
    "prod": "cross-env NODE_ENV=production tailwindcss -i ./css/source.css -o ./css/apple-notes.css --postcss --minify"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  }
}




